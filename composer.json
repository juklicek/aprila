{
	"name": "aprila/aprila",
	"description": "Aprila: useful PHP code for simple administration based on Nette Framework",
	"keywords": ["admin", "registration", "login", "nette"],
	"homepage": "https://github.com/Aprila",
	"license": ["MIT"],
	"authors": [
		{
			"name": "Honza Cerny",
			"homepage": "http://honzacerny.com"
		}
	],
	"require": {
		"php": ">=8.2",
		"nette/application": "3.1.*",
		"nette/bootstrap": "3.1.*",
		"nette/caching": "3.2.*",
		"nette/database": "3.1.*",
		"nette/forms": "3.1.*",
		"nette/http": "3.2.*",
		"nette/security": "3.1.*"
	},
	"require-dev": {
		"nette/tester": "1.2"
	},
	"autoload": {
		"classmap": ["src/"],
		"files": ["src/shortcuts.php"]
	}
}
